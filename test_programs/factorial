function factorial(n) do
    if n = 0 do
        n := 1
        return
    if n = 1 do
        n := 1
        return
    k := n - 1
    factorial(k)
    n := n * k
    return
done



function factorial(n) do # param: n = [n], return: n = [n, result]
    if n(0) < 2 do
        n(1) = 1
        return
    k := n - 1
    factorial(k)
    n(1) := n(1) * k(1)
    return
done



function main do
    int n := 5
    factorial(n)
    
    